{
  "secrets": {
    "path": {
      "production": "",
      "development": "./secrets.json"
    }
  },
  "database": {
    "path": {
      "production": "",
      "development": "./database/"
    }
  },
  "execution": {
    "ttl": {
      "volumes": 1800000,
      "vms": 1800000
    },
    "vms": {
      "sandboxDirectory": {
        "production": "",
        "development": "./sandbox/"
      }
    },
    "gcInterval": 600000
  },
  
  "dbPort": 8001,
  "adminPort": 5000,
  "executePort": 8000,

  "maxWorkerProcesses": 16,

  "ssl": {
    "enabled": false,
    "keyPath": {
      "production": "",
      "development": ""
    },
    "certPath": {
      "production": "",
      "development": ""
    }
  }
}